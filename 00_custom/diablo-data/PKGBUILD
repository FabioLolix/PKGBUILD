pkgname=diablo-data
pkgver=1
pkgrel=1
pkgdesc="Diablo data for Freeablo"
arch=(any)
url=""
license=('Blizzard')
source=("local://DIABDAT.MPQ"
        "local://Diablo.exe")
sha256sums=('63fb47d9c76484024c7640d90ab6b7ec5e13f567a7e1a917b6c03a6631d3f2b0'
            '66ccafdd6d33925e00645afd56e2999fce3932e198e6dfde66f055ece04f227e')

package() {

  install -D DIABDAT.MPQ -t "$pkgdir"/usr/share/freeablo/resources/
  install -D Diablo.exe -t "$pkgdir"/usr/share/freeablo/resources/
}
